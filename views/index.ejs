<!DOCTYPE html>
<html>
<head>
  <title>Employee List</title>
  <style>
    /* CSS styles */
    body {
      font-family: Arial, sans-serif;
      background-color: #f1f1f1;
      padding: 20px;
    }
    
    h1 {
      color: #333333;
    }
    
    ul {
      list-style-type: none;
      padding: 0;
    }
    
    li {
      margin-bottom: 10px;
    }
    
    a {
      margin-left: 10px;
      color: #333333;
      text-decoration: none;
    }
    
    a:hover {
      text-decoration: underline;
    }
  </style>
</head>
<body>
  <h1>Employee List</h1>
  <ul>
    <% employees.forEach((employee) => { %>
      <li>
        <strong><%= employee.name %></strong> - <%= employee.designation %>
        <a href="/employees/<%= employee.id %>/edit">Edit</a>
        <a href="/employees/<%= employee.id %>/delete">Delete</a>
      </li>
    <% }); %>
  </ul>
  <a href="/employees/add">Add Employee</a>
</body>
</html>
